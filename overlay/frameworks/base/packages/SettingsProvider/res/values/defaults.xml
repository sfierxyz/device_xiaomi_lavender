<resources>
    
    <!-- Disable mobile data by default -->
    <bool name="def_mobile_data_always_on">false</bool>
    
    <!-- Disable bluetooth by default -->
    <bool name="def_bluetooth_on">false</bool>
    
    <!-- Disable charging sex toys by default -->  
    <bool name="def_charging_vibration_enabled">false</bool>  

    <!-- Disable charging sounds by default -->  
    <bool name="def_charging_sounds_enabled">false</bool>   

    <!-- Enable notification bubbles by default -->
    <bool name="def_notification_bubbles">true</bool>
</resources>
